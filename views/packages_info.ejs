<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="container mt-4">
      <table class="table">
        <thead>
          <tr>
            <th>package number</th>
            <th>weight</th>
            <th>destination</th>
            <th>dimensions</th>
            <th>category</th>
            <th>insurance amount</th>
            <th>delivery_date</th>
          </tr>
        </thead>
        <tbody>
          <% if(data.length){ for(var i = 0; i< data.length; i++) {%>
          <tr>
            <td><%= data[i].package_number%></td>
            <td><%= data[i].weight%></td>
            <td><%= data[i].destination%></td>
            <td><%= data[i].dimensions%></td>
            <td><%= data[i].category%></td>
            <td><%= data[i].insurance_amount%></td>
            <td><%= data[i].delivery_date%></td>
          </tr>
          <% } }else{ %>
          <tr>
            <td>No data ever existed.</td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>